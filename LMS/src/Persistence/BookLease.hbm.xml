<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Persistence.BookLease" table="book_lease" schema="public" catalog="LibraryManagementSystem">
        <id name="leaseId" column="lease_id"/>
        <property name="leaseDate" column="lease_date"/>
        <property name="dueDate" column="due_date"/>
        <property name="renewed" column="renewed"/>
        <many-to-one name="bookEntityByBookEntityId" class="Persistence.BookEntity">
            <column name="book_entity_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="employeeByEmployeeId" class="Persistence.Employee">
            <column name="employee_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="memberByMemberId" class="Persistence.Member">
            <column name="member_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
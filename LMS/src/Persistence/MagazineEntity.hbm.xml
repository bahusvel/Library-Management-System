<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Persistence.MagazineEntity" table="magazine_entity" schema="public" catalog="LibraryManagementSystem">
        <id name="magEntityId" column="mag_entity_id"/>
        <property name="available" column="available"/>
        <many-to-one name="magazineEditionByMagazineEditionId" class="Persistence.MagazineEdition">
            <column name="magazine_edition_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
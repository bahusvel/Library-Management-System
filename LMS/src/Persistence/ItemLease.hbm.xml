<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Persistence.ItemLease" table="item_lease" schema="public" catalog="LibraryManagementSystem">
        <id name="itemLeaseId" column="item_lease_id"/>
        <property name="leaseDate" column="lease_date"/>
        <property name="dueDate" column="due_date"/>
        <property name="renewed" column="renewed"/>
        <many-to-one name="employeeByEmployeeId" class="Persistence.Employee">
            <column name="employee_id"/>
        </many-to-one>
        <many-to-one name="itemEntityByItemEntityId" class="Persistence.ItemEntity">
            <column name="item_entity_id"/>
        </many-to-one>
        <many-to-one name="memberByMemberId" class="Persistence.Member">
            <column name="member_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>
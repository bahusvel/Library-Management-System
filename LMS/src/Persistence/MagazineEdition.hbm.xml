<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Persistence.MagazineEdition" table="magazine_edition" schema="public"
           catalog="LibraryManagementSystem">
        <id name="magEditionId" column="mag_edition_id"/>
        <property name="editionDate" column="edition_date"/>
        <property name="editionTitle" column="edition_title"/>
        <many-to-one name="magazineByMagazineId" class="Persistence.Magazine">
            <column name="magazine_id" not-null="true"/>
        </many-to-one>
        <set name="magazineEntitiesByMagEditionId" inverse="true">
            <key>
                <column name="magazine_edition_id" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="Persistence.MagazineEntity"/>
        </set>
    </class>
</hibernate-mapping>